    "type": "module",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --mode development --open --hot",
        "build": "webpack --mode production",
        "server:start:ts-node-2": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./src/server/index.ts",
        "server:start:ts-node": "ts-node -P tsconfig.server.json ./src/server/index.ts",
        "server:start:tsx": "tsx --watch --tsconfig tsconfig.server.json ./src/server/index.ts",
        "server:build": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "@babel/runtime": "^7.23.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@types/node": "^20.11.20",
        "@types/react": "^18.2.59",
        "@types/react-dom": "^18.2.19",
        "babel-loader": "^9.1.3",
        "html-webpack-plugin": "^5.6.0",
        "jest": "^29.7.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "webpack": "^5.90.3",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.2"
    }
}
